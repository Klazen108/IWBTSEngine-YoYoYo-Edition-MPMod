var deals_damage=true;
with (other) {
    //in multiplayer mode, don't deal damage with the bullet if it's not a local instance
    //(when one player shoots a bullet, one will be created on everyone else's computers as well
    // we only want the orignal bullet to deal damage, not all the copies as well)
    with (player) if (scrIsMultiplayerMode()) if (!htme_isLocal()) deals_damage=false;
    instance_destroy();
}

if (deals_damage &amp;&amp; scrIsMultiplayerMode()) {
    currentHP = htme_globalGet("boss_hp");
    if (is_undefined(currentHP)) {
        currentHP=20;
    }
    currentHP-=1;
    htme_globalSet("boss_hp",currentHP,buffer_u32);
} else {
    currentHP -= 1;
}

